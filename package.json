{
  "name": "iviche-back",
  "version": "1.0.0",
  "engines": {
    "node": "12.14"
  },
  "description": "Back-end part of iViche portal",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p tsconfig.json",
    "build:live": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "prod": "cross-env NODE_ENV=production node ./build/src/index.js",
    "start-cron-job": "cross-env NODE_ENV=production node ./build/src/cronJobs.js",
    "test": "cross-env NODE_ENV=test TEST_MODE=unit jest --verbose",
    "test:i9n": "cross-env NODE_ENV=test TEST_MODE=i9n jest --verbose --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand",
    "lint": "eslint \"./src/**/*.ts\" \"./test/**/*.ts\"",
    "lint:fix": "eslint \"./src/**/*.ts\" \"./test/**/*.ts\" --fix",
    "migrate:latest": "knex --knexfile=./database/knexfile.ts migrate:latest",
    "seed:testdb": "ts-node ./database/seeds/02_TestData.ts",
    "cleanup:storage": "ts-node ./database/cleanupDatabaseAndElastic.ts",
    "index:polls": "ts-node ./database/indexPolls.ts"
  },
  "author": "DeWais LLC Engineering",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/bluebird": "3.5.32",
    "@types/body-parser": "1.17.1",
    "@types/busboy": "0.2.3",
    "@types/cheerio": "0.22.18",
    "@types/common-tags": "1.8.0",
    "@types/cookie": "0.3.3",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/cron": "1.7.1",
    "@types/express": "4.17.1",
    "@types/express-winston": "3.0.4",
    "@types/hapi__joi": "15.0.4",
    "@types/jest": "25.2.1",
    "@types/joi-password-complexity": "3.1.0",
    "@types/jsonwebtoken": "8.3.4",
    "@types/luxon": "1.21.0",
    "@types/mock-knex": "0.4.1",
    "@types/node": "12.7.4",
    "@types/nodemailer": "6.2.1",
    "@types/request-ip": "0.0.35",
    "@types/supertest": "2.0.8",
    "@types/ua-parser-js": "0.7.33",
    "@types/uuid": "3.4.5",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "cross-env": "6.0.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "4.0.0",
    "eslint-plugin-sort-class-members": "1.6.0",
    "husky": "3.0.8",
    "jest": "24.9.0",
    "mock-knex": "0.4.6",
    "node-mocks-http": "1.8.0",
    "nodemon": "2.0.4",
    "prettier": "1.19.1",
    "reflect-metadata": "0.1.13",
    "simple-git": "1.126.0",
